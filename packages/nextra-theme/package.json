  },
  "devDependencies": {
    "@edgeandnode/gds": "^5.11.2",
    "@edgeandnode/go": "^6.17.0",
    "@emotion/react": "^11",
    "@types/lodash": "^4",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "next": "^14",
    "next-seo": "^6",
    "nextra": "^2",
