  "devDependencies": {
    "@edgeandnode/gds": "^5.23.0",
    "@edgeandnode/go": "^6.33.0",
    "@emotion/react": "^11.12.0",
    "@types/lodash": "^4.17.7",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
