{
  "name": "@graphprotocol/docs",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "next",
    "predev": "pnpm prebuild",
    "build": "rm -rf .next && rm -rf out && next build",
    "prebuild": "tsup ./src/i18n.ts --format esm && pnpm fetch-remote-docs && pnpm fix-pages-structure",
    "postbuild": "next-sitemap --config next-sitemap.config.mjs && node scripts/sitemap-ci.js",
    "typecheck": "tsc --noEmit",
    "fetch-remote-docs": "tsx scripts/fetch-remote-docs.ts",
    "fix-pages-structure": "tsx scripts/fix-pages-structure.ts",
    "move-pages": "tsx scripts/move-pages.ts"
  },
  "dependencies": {
    "@docsearch/react": "^3.8.2",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-visually-hidden": "^1.1.1",
    "lodash": "^4.17.21",
    "react-intersection-observer": "^9.14.1",
    "react-use": "^17.6.0",
    "@edgeandnode/common": "^6.43.0",
    "@edgeandnode/gds": "^5.41.0",
    "@edgeandnode/go": "^6.79.0",
    "@emotion/react": "^11.14.0",
    "@graphprotocol/contracts": "6.2.1",
    "mixpanel-browser": "^2.58.0",
    "next": "^14.2.23",
    "next-seo": "^6.6.0",
    "next-sitemap": "^4.2.3",
    "nextra": "^3.3.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-ga4": "^2.1.0",
    "theme-ui": "^0.17.1",
    "unist-util-visit": "^5.0.0"
  },
  "devDependencies": {
    "@types/lodash": "^4.17.14",
    "@graphprotocol/client-cli": "3.0.7",
    "@types/mdast": "^4.0.4",
    "@types/mixpanel-browser": "^2.51.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "autoprefixer": "^10.4.20",
    "fast-xml-parser": "^4.5.1",
    "graphql": "^16.10.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "tsup": "^8.3.5",
    "unified": "^11.0.5"
  }
}
